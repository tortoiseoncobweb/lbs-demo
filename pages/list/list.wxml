<!--list.wxml-->

<scroll-view scroll-y="true">
  <view wx:for="{{diaries}}" wx:for-index="idx" class="item-container" bindtap="showDetail" id="{{idx}}" data-identify="{{item.identify}}">
  
    <view class="desc">
      <view class="left">
        <view style="font-size:32rpx;margin:10rpx 0;">{{item.callout.content}}</view>
        
      </view>
      <view class="right">
        <image mode="aspectFit" src="{{item.iconPath}}"></image>
     
      </view>
    </view>
  </view>
 <view class='dixian'>-----这位客官，换个地方看看吧-----</view>
</scroll-view>
